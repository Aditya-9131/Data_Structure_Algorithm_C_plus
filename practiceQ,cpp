# include<iostream>
using namespace std;
int square(int num){
    return num*num;
}
void firstNsquare(int n){
    for(int i=1; i<=n; i++){
        cout<<i<<" "<<square(i)<<endl;
    }
}
int main(){
    int n;
    cout<<"Enter the number : ";
    cin>>n;
    firstNSquares(n);
    return 0;
}